skill_level <- "Advanced"
}
cat("\nStatistics:\n")
cat("Number of words:", num_words, "\n")
cat("Number of characters:", num_chars, "\n")
cat("Average word length:", round(avg_word_length, 2), "\n")
cat("\nCommunication Skill Level:", skill_level, "\n")
}
